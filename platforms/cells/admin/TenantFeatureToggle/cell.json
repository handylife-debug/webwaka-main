    "toggleFeature": {
      "type": "object",
      "properties": {
        "success": { "type": "boolean" },
        "message": { "type": "string" },
        "updatedFeatures": { "type": "array" }
      }
    }
  },
  "actions": ["renderFeatureManager", "toggleFeature", "getFeatureTemplates", "applyFeatureTemplate"],
